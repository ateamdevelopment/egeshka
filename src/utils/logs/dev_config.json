{
  "version": 1,
  "disable_existing_loggers": true,
  "formatters": {
    "werkzeug_formatter": {
      "class": "src.utils.logs.formatters.WrapFormatter",
      "datefmt": "[%d.%m-%H:%M:%S]",
      "format": "web info: %(message)s"
    },
    "root_formatter": {
      "class": "src.utils.logs.formatters.WrapFormatter",
      "datefmt": "[%d.%m-%H:%M:%S]",
      "format": "%(levelname)-5s| %(name)s %(asctime)s <%(module)s->%(funcName)s(%(lineno)d)>: %(message)s"
    }
  },
  "handlers": {
    "werkzeug_console_handler": {
      "level": "DEBUG",
      "class": "logging.StreamHandler",
      "formatter": "werkzeug_formatter",
      "stream": "ext://sys.stdout"
    },
    "root_console_handler": {
      "level": "DEBUG",
      "class": "src.utils.logs.handlers.StdHandler",
      "formatter": "root_formatter"
    }
  },
  "loggers": {
    "werkzeug": {
      "handlers": ["werkzeug_console_handler"],
      "level": "DEBUG",
      "propagate": false
    },
    "root": {
      "handlers": ["root_console_handler"],
      "level": "DEBUG"
    }
  }
}